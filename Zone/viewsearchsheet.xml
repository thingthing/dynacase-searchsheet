
[IF NEEDLIMIT]<table style="width:100%"><tr><td>
<select id="ilimit" title="[TEXT:Limit of rows]" onchange="resetReport()">
[BLOCK optlimit]<option [optselected] value="[optval]">[optlabel]</option>[ENDBLOCK optlimit]
</select>
<a id="aprev" style="visibility:hidden" title="[TEXT:Previous page]" onclick="viewReportPage(-1)"><img align="absbottom" src="[IMG:prev.png]"></a><input disabled type="text" size="3" value="1" id="ipage" onkeypress="if (trackCR(event)) resetReport()"><a title="[TEXT:Next page]" id="anext"  onclick="viewReportPage(1)"><img align="absbottom" src="[IMG:next.png]"></a></td><td></td></tr></table>[ENDIF NEEDLIMIT]


<input type="hidden" value="[ID]" id="sheetid">
<div class="action">
[BLOCK ACTIONS]
<a  target="[tgt]" onclick="sendReportAction(event,'[CORE_STANDURL]&app=[app]&action=[act]&[arg]','[aid]')">[lbl]</a>
[ENDBLOCK ACTIONS]</div>



<div id="report">
[thereport]
</div>

[IF NEEDLIMIT]<script>LIMIT=true;</script>[ENDIF NEEDLIMIT]
