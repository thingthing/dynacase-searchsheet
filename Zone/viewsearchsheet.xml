
[IF NEEDLIMIT]
<div class="inputarea">
<select  id="ilimit" title="[TEXT:Limit of rows]" onchange="resetReport()">[BLOCK optlimit]<option [optselected] value="[optval]">[optlabel]</option>[ENDBLOCK optlimit]</select>
<a id="aprev" style="visibility:hidden" title="[TEXT:Previous page]" onclick="viewReportPage(-1)"><img align="absbottom" src="[IMG:ssheet-prev.gif]"></a><input disabled type="text" size="3" value="1" id="ipage" onkeypress="if (trackCR(event)) resetReport()"><a title="[TEXT:Next page]" id="anext"  onclick="viewReportPage(1)"><img align="absbottom" src="[IMG:ssheet-next.gif]"></a>
</div>
[ENDIF NEEDLIMIT]
<input type="hidden" value="[ID]" id="sheetid">
<div class="action">
[BLOCK ACTIONS]
<a  target="[tgt]" onclick="sendReportAction(event,'[CORE_STANDURL]&app=[app]&action=[act]&[arg]','[aid]')">[lbl]</a>[ENDBLOCK ACTIONS]
<a title="[TEXT:Reset filters]" onclick="resetFilters(event)">[TEXT:ResetSearch]</a>
<a target="_self" title="[TEXT:ImportCsvResult]" onclick="sendReportAction(event,'[CORE_STANDURL]&app=FDL&action=VIEWSCARD&id=[ID]&zone=SEARCHSHEET:CSVSEARCHSHEET','docids')">[TEXT:Selected Csv]</a>
<a target="_self" title="[TEXT:ImportAllCsvResult]" onclick="sendReportAction(event,'[CORE_STANDURL]&app=FDL&action=VIEWSCARD&id=[ID]&zone=SEARCHSHEET:CSVSEARCHSHEET&limit=ALL','docids')">[TEXT:All Csv]</a>
</div>


<div class="report" id="report">
[thereport]
</div>

[IF NEEDLIMIT]<script>LIMIT=true;</script>[ENDIF NEEDLIMIT]
